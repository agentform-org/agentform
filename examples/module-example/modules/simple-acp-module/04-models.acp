// Model Configuration
// Provides pre-configured models for different use cases

model "default" {
  provider = provider.llm.openai.default
  id       = var.default_model
  
  params {
    temperature = 0.7
    max_tokens  = 2000
  }
}

model "creative" {
  provider = provider.llm.openai.default
  id       = "gpt-4o"
  
  params {
    temperature = 0.9
    max_tokens  = 4000
  }
}

model "precise" {
  provider = provider.llm.openai.default
  id       = "gpt-4o"
  
  params {
    temperature = 0.1
    max_tokens  = 2000
  }
}
